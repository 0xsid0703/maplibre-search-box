{
  "name": "@stadiamaps/maplibre-search-box",
  "version": "0.2.1",
  "homepage": "https://docs.stadiamaps.com/",
  "repository": "https://github.com/stadiamaps/maplibre-search-box",
  "license": "BSD-3-Clause",
  "main": "./dist/maplibre-search-box.umd.js",
  "module": "./dist/maplibre-search-box.mjs",
  "style": "dist/style.css",
  "files": [
    "dist"
  ],
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/maplibre-search-box.umd.js",
      "import": "./dist/maplibre-search-box.mjs"
    },
    "./dist/*.css": {
      "require": "./dist/*.css",
      "import": "./dist/*.css"
    }
  },
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint:scripts": "eslint . --ext .ts",
    "lint:styles": "stylelint ./**/*.{css,scss}",
    "format:scripts": "prettier . --write",
    "format:styles": "stylelint ./**/*.{css,scss} --fix",
    "format": "npm run format:scripts && npm run format:styles",
    "prepare": "husky install && husky set .husky/pre-commit 'npx lint-staged' && git add .husky/pre-commit",
    "uninstall-husky": "npm uninstall husky --no-save && git config --unset core.hooksPath && npx rimraf .husky"
  },
  "devDependencies": {
    "@types/jsdom": "^20.0.1",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "@vitest/browser": "^0.32.2",
    "@vitest/coverage-c8": "^0.32.2",
    "@wdio/types": "^8.10.4",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "jsdom-worker": "^0.3.0",
    "lint-staged": "^13.1.0",
    "postcss": "^8.4.21",
    "postcss-scss": "^4.0.6",
    "prettier": "^2.8.3",
    "sass": "^1.63.3",
    "stylelint": "^14.16.1",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recommended": "^9.0.0",
    "stylelint-config-sass-guidelines": "^9.0.0",
    "stylelint-prettier": "^3.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3",
    "vite": "^4.0.4",
    "vite-plugin-dts": "^2.3.0",
    "vitest": "^0.32.2",
    "webdriverio": "^8.11.2"
  },
  "peerDependencies": {
    "maplibre-gl": ">=2.0.0"
  },
  "dependencies": {
    "@stadiamaps/api": "^1.0.4"
  }
}
